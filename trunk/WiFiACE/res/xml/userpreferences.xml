	<PreferenceCategory android:orderingFromXml="true" android:title="@string/TITLE_KEYS">
	<EditTextPreference android:key="WPA_KEY" android:title="@string/WPA_KEY" android:password="true"></EditTextPreference>
		<PreferenceScreen android:title="@string/TITLE_WEP_KEYS">

			<ListPreference android:entryValues="@array/wepkeyidx_values" android:title="@string/KEY_IDX" android:entries="@array/wepkeyidx_names" android:key="KEY_IDX"></ListPreference>
			<EditTextPreference android:title="@string/KEY_KEY0"
				android:key="KEY_KEY0" android:password="true"></EditTextPreference>
			<EditTextPreference android:title="@string/KEY_KEY1"
				android:key="KEY_KEY1" android:password="true"></EditTextPreference>
			<EditTextPreference android:title="@string/KEY_KEY2"
				android:key="KEY_KEY2" android:password="true"></EditTextPreference>
			<EditTextPreference android:title="@string/KEY_KEY3"
				android:key="KEY_KEY3" android:password="true"></EditTextPreference>
		
</PreferenceScreen>
	
</PreferenceCategory>

</PreferenceScreen>
